<div class="modal fade see_injury_analysis" id="see_injury_analysis" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header modal-header-primary">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true"><i class="fa fa-times-circle fa-lg"></i></span></button>
        <h4 class="modal-title panel-primary" id="myModalLabel">Biblioteca de analisis de lesiónes</h4>
      </div>
      <div class="modal-body">
        <div class="container">
          <div class="row">
            <div class="col-sm-3 col-md-6">
              <div id="imaginary_container"> 
                <div class="input-group stylish-input-group">
                  <input type="search" class="form-control" placeholder="Ingrese el nombre de la lesión" ng-model="search_injury.$" size="50" >
                  <span class="input-group-addon">
                    <span class="glyphicon glyphicon-search"></span>    
                  </span>
                </div>
            </div>
              <br>
              <div class="panel-group" id="accordion" style="width: 100%; height: 530px; overflow-y: scroll;">
                <div class="panel panel-default" id="analysis_injury{{injury.id}}" ng-repeat="injury in cCtlr.library_injury | filter:search_injury:strict">
                  <div class="panel-heading">
                    <h4 class="panel-title">
                      <a data-parent="#accordion{{injury.id}}">
                      </span>{{injury.name}}</a>
                    </h4>
                  </div>
                  <div id="collapse" class="panel-collapse collapse in">
                    <div class="panel-body">
                      <div class="col-md-10">
                        <h6 class="title">{{injury.case_analysis}}</h6>
                      </div>
                      <div class="col-md-2">
                        <button type="button" class="btn btn-primary" ng-click="cCtlr.ChargeInjuryAnalysis(injury.id)">Cargar</button>
                        <button type="button" class="btn btn-primary" ng-click="cCtlr.DeleteInjuryAnalysis(injury.id)"" style="margin-top:3px;width:69px;">Borrar</button>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<script>
  $(document).ready(function(){
    var height_windows = $( window ).height() - 356;
    $('.scroll_to_terms').css({'height': height_windows});
  })
</script>